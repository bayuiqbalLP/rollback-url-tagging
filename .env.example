# Database connection string (MySQL DSN format)
# Example: username:password@tcp(host:port)/database?parseTime=true
DB_DSN="user:pass@tcp(127.0.0.1:3306)/genesis?parseTime=true"

# Use DRY_RUN=1 to log changes without updating the database
DRY_RUN=0

# Optional batch size override (defaults to 200 if unset)
BATCH_SIZE=200

# Hydra asset signing prefix; script only touches URLs with this prefix
HYDRA_SIGN_PREFIX="https://api.dev-genesis.lionparcel.com/hydra/v1/asset/sign?"
# AWS Custom Rate URL 
BULK_S3_PREFIX=https://dev-genesis.s3.ap-southeast-1.amazonaws.com/